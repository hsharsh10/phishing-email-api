[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        async function analyzeEmail() {[m
[32m+[m[32m            const emailText = document.getElementById('emailText').value.trim();[m
[32m+[m[32m            const analyzeBtn = document.getElementById('analyzeBtn');[m
[32m+[m[32m            const resultsContainer = document.getElementById('resultsContainer');[m
[32m+[m[32m            const errorContainer = document.getElementById('errorContainer');[m
[32m+[m
[32m+[m[32m            // Clear previous results[m
[32m+[m[32m            resultsContainer.classList.add('hidden');[m
[32m+[m[32m            errorContainer.classList.add('hidden');[m
[32m+[m
[32m+[m[32m            // Validation[m
[32m+[m[32m            if (!emailText) {[m
[32m+[m[32m                showError('Please enter email content to analyze.');[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Show loading state[m
[32m+[m[32m            analyzeBtn.disabled = true;[m
[32m+[m[32m            analyzeBtn.innerHTML = '<span class="spinner"></span>Analyzing...';[m
[32m+[m
[32m+[m[32m            try {[m
[32m+[m[32m                const response = await fetch(API_ENDPOINT, {[m
[32m+[m[32m                    method: 'POST',[m
[32m+[m[32m                    headers: {[m
[32m+[m[32m                        'Content-Type': 'application/json'[m
[32m+[m[32m                    },[m
[32m+[m[32m                    body: JSON.stringify({[m
[32m+[m[32m                        email_text: emailText[m
[32m+[m[32m                    })[m
[32m+[m[32m                });[m
[32m+[m
[32m+[m[32m                if (!response.ok) {[m
[32m+[m[32m                    throw new Error(`HTTP error! status: ${response.status}`);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                const data = await response.json();[m
[32m+[m[32m                displayResults(data);[m
[32m+[m
[32m+[m[32m            } catch (error) {[m
[32m+[m[32m                showError(`Analysis failed: ${error.message}. Please ensure the API server is running.`);[m
[32m+[m[32m            } finally {[m
[32m+[m[32m                // Reset button[m
[32m+[m[32m                analyzeBtn.disabled = false;[m
[32m+[m[32m                analyzeBtn.innerHTML = 'Analyze Email';[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function displayResults(data) {[m
[32m+[m[32m            const resultsContainer = document.getElementById('resultsContainer');[m
[32m+[m[32m            const isPhishing = data.prediction === 'PHISHING';[m
[32m+[m[32m            const confidencePercent = (data.confidence * 100).toFixed(1);[m
[32m+[m
[32m+[m[32m            resultsContainer.innerHTML = `[m
[32m+[m[32m                <div class="results">[m
[32m+[m[32m                    <div class="result-header">[m
[32m+[m[32m                        <div class="result-label ${isPhishing ? 'result-phishing' : 'result-legit'}">[m
[32m+[m[32m                            <span class="result-icon">[m
[32m+[m[32m                                ${isPhishing ?[m
[32m+[m[32m                    '<svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m21.73 18-8-14a2 2 0 0 0-3.48 0l-8 14A2 2 0 0 0 4 21h16a2 2 0 0 0 1.73-3Z"></path><line x1="12" y1="9" x2="12" y2="13"></line><line x1="12" y1="17" x2="12.01" y2="17"></line></svg>' :[m
[32m+[m[32m                    '<svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M22 11.08V12a10 10 0 1 1-5.93-9.14"></path><polyline points="22 4 12 14.01 9 11.01"></polyline></svg>'}[m
[32m+[m[32m                            </span>[m
[32m+[m[32m                            <span>${data.prediction}</span>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                    </div>[m
[32m+[m[41m                    [m
[32m+[m[32m                    <div class="confidence-container">[m
[32m+[m[32m                        <div class="confidence-label">[m
[32m+[m[32m                            <span>Confidence Level</span>[m
[32m+[m[32m                            <span class="confidence-value">${confidencePercent}%</span>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                        <div class="confidence-bar">[m
[32m+[m[32m                            <div class="confidence-fill ${isPhishing ? 'confidence-phishing' : 'confidence-legit'}"[m[41m [m
[32m+[m[32m                                 style="width: ${confidencePercent}%">[m
[32m+[m[32m                            </div>[m
[32m+[m[32m                        </div>[m
[32m+[m[32m                    </div>[m
[32m+[m[32m                </div>[m
[32m+[m[32m            `;[m
[32m+[m
[32m+[m[32m            resultsContainer.classList.remove('hidden');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function showError(message) {[m
[32m+[m[32m            const errorContainer = document.getElementById('errorContainer');[m
[32m+[m[32m            errorContainer.innerHTML = `[m
[32m+[m[32m                <div class="error">[m
[32m+[m[32m                    <span class="error-icon">[m
[32m+[m[32m                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="8" x2="12" y2="12"></line><line x1="12" y1="16" x2="12.01" y2="16"></line></svg>[m
[32m+[m[32m                    </span>[m
[32m+[m[32m                    <span>${message}</span>[m
[32m+[m[32m                </div>[m
[32m+[m[32m            `;[m
[32m+[m[32m            errorContainer.classList.remove('hidden');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function clearForm() {[m
[32m+[m[32m            document.getElementById('emailText').value = '';[m
[32m+[m[32m            document.getElementById('resultsContainer').classList.add('hidden');[m
[32m+[m[32m            document.getElementById('errorContainer').classList.add('hidden');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function loadExample(type) {[m
[32m+[m[32m            document.getElementById('emailText').value = examples[type];[m
[32m+[m[32m            document.getElementById('resultsContainer').classList.add('hidden');[m
[32m+[m[32m            document.getElementById('errorContainer').classList.add('hidden');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Keyboard shortcut: Ctrl/Cmd + Enter to analyze[m
[32m+[m[32m        document.getElementById('emailText').addEventListener('keydown', function (e) {[m
[32m+[m[32m            if ((e.ctrlKey || e.metaKey) && e.key === 'Enter') {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                analyzeEmail();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Toggle About section[m
[32m+[m[32m        function toggleAbout() {[m
[32m+[m[32m            const content = document.getElementById('aboutContent');[m
[32m+[m[32m            const icon = document.getElementById('toggleIcon');[m
[32m+[m
[32m+[m[32m            content.classList.toggle('open');[m
[32m+[m[32m            icon.classList.toggle('open');[m
[32m+[m[32m        }[m
